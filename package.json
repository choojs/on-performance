  "devDependencies": {
    "bankai": "^8.0.0",
    "browserify": "^14.4.0",
    "dependency-check": "^2.8.0",
    "nanotiming": "^5.2.1",
    "standard": "^10.0.2",
    "tape": "^4.6.3",
    "tape-run": "^3.0.0"
  },
  "keywords": [
    "performance",
    "timeline",
    "api"
  ],
  "license": "MIT"
}
